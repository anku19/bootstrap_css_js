<p id="notice"><%= notice %></p>

<div class="page-header">
<h1>All Todo's</h1>
</div>
    <% @notes.each do |note| %>
      
       <h3> <%= note.notes %>
        <%= link_to '<i class="fa fa-pencil" aria-hidden="true"></i></i>'.html_safe, edit_note_path(note) %>
        <%= link_to '<i class="fa fa-trash" aria-hidden="true"></i>'.html_safe, note, method: :delete, data: { confirm: 'Are you sure?' } %></h3>
      
    <% end %>


<br>

<%= link_to 'New Note', new_note_path %>
